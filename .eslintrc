{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": 0,
    "object-curly-newline": "off",
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id", "_baseUrl", "_headers"] }
    ],
    "no-console": "off",
    "comma-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
